{
  "name": "foresee",
  "description": "Foresee Planning",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
     "express": "3.x"
    ,"socket.io": "0.9.x"
    ,"coffee-script": "1.4.0"
    ,"connect-assets": "2.3.3"
    ,"which": "1.0.5"
    ,"wrench": "1.4.4"
    ,"ect":"0.4.x"
    ,"memory-cache":"0.0.x"
    ,"selenium-webdriver":"2.31.0"
  }, "devDependencies": {
      "coffee-script": "*"
    , "mocha": "*"
    , "should": "*"
    , "supervisor": "*"
    , "chai": "1.5.x"
    , "sinon": "1.6.x"
  },
  "scripts": {
    "dev": "supervisor -w src -e 'js|coffee' app"
   ,"compile": "coffee -c -o build/ src/ test/"
   ,"autotest": "mocha --reporter min --watch --compilers coffee:coffee-script --colors test/*.coffee" 
   ,"test": "mocha --reporter spec --compilers coffee:coffee-script --colors --require should test/*.coffee"
   ,"webtest": "mocha --reporter spec --compilers coffee:coffee-script --colors --require should webtest/*.coffee -t 30000"
   ,"browsertest": "coffee -c -o browsertest/.js/ -w browsertest/"
  }
}
